<?xml version="1.0" encoding="UTF-8"?>
<window><!-- Aeon MQ 9 -->
	<controls>
		<control type="group">
			<width>3840</width>
			<height>600</height>
			<animation effect="slide" time="400" start="0,-600" end="0,0" easing="out" tween="sine">WindowOpen</animation>
			<animation effect="slide" time="400" start="0,-600" end="0,0" easing="out" tween="sine">Visible</animation>
			<animation effect="slide" time="400" start="0,0" end="0,-600" easing="in" tween="sine">WindowClose</animation>
			<animation effect="slide" time="400" start="0,0" end="0,-600" easing="in" tween="sine">Hidden</animation>
			<visible>![Window.IsVisible(seekbar) | Window.IsVisible(osdvideosettings) | Window.IsVisible(osdaudiosettings) | Window.IsActive(fullscreeninfo) | Window.IsActive(videoosd) | Skin.HasSetting(customosd)]</visible>
			<control type="image">
				<height>600</height>				
				<texture flipy="true" colordiffuse="$INFO[skin.string(texture_base_color)]">shadow_mask.png</texture>
			</control>
			<control type="grouplist">
				<left>100</left>
				<top>200</top>
				<width>3640</width>
				<height>600</height>
				<itemgap>14</itemgap>
				<control type="label" id="10">
				</control>
				<control type="label" id="11">
				</control>
				<control type="label" id="12">
				</control>
			</control>
		</control>
	</controls>
</window>