<?xml version="1.0" encoding="UTF-8"?>
<window><!-- Aeon MQ 9 -->
	<defaultcontrol always="true">9010</defaultcontrol>
	<onload condition="String.IsEqual(skin.string(submenu),00) | String.IsEqual(Container(9010).numitems,0)">SetFocus(9011)</onload>
	<onunload>Skin.Reset(submode)</onunload>
	<onunload>SetProperty(background_copy,$INFO[Container(9000).ListItem.Icon],home)</onunload>
	<controls>
		<control type="group">
			<include condition="Window.IsVisible(1157)">menucustom</include>
			<animation effect="fade" start="0" end="100" time="0" condition="Skin.HasSetting(submode)">Conditional</animation>
		</control>		
		<control type="group">
			<include condition="Window.IsVisible(home)">animation_dialog_popup</include>
			<animation type="WindowOpen" reversible="false" condition="Window.IsVisible(1157)">
				<effect type="slide" start="0,0" end="900,0" time="300" delay="300" tween="sine" easing="inout" />
				<effect type="fade" start="0" end="100" time="300" />
			</animation>
			<animation type="WindowClose" reversible="false" condition="Window.IsVisible(1157)">
				<effect type="slide" start="900,0" end="0,0" time="300" tween="sine" easing="inout" />
				<effect type="fade" start="100" end="0" time="300" />
			</animation>
			<animation effect="slide" end="0,600" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,1) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,556" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,2) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,512" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,3) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,468" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,4) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,424" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,5) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,380" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,6) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,336" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,7) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,292" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,8) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,248" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,9) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,204" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,10) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,160" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,11) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,116" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,12) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,72" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,13) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="0,28" time="200" easing="inout" tween="sine" condition="String.IsEqual(Container(9010).numitems,14) + Window.IsVisible(home)">Conditional</animation>
			<animation effect="slide" end="-752,400" time="0" tween="sine" easing="out" condition="Window.IsVisible(home) + [String.IsEqual(skin.string(submenu),00) | String.IsEqual(Container(9010).numitems,0)]">Conditional</animation>
			<include content="context_furniture">	
				<param name="left" value="490" />
				<param name="top" value="250" />
				<param name="label" value="$INFO[Window(home).Property(menu_item_label)]/$LOCALIZE[31201]" />
				<param name="visible" value="Window.IsVisible(home) + ![String.IsEqual(skin.string(submenu),00) | String.IsEqual(Container(9010).numitems,0)]" />
			</include>
			<control type="list" id="9010">
				<include content="submenu_list" condition="Window.IsVisible(home)">	
					<param name="id" value="9010" />
					<param name="left" value="490" />
					<param name="top" value="250" />
					<param name="width" value="1362" />
					<param name="height" value="2010" />
					<param name="onleft" value="Dialog.Close(submenu)" />
					<param name="onright" value="9011" />
					<param name="align" value="center" />
					<param name="focusposition" value="0" />
					<param name="itemwidth" value="1362" />
					<param name="itemheight" value="90" />
				</include>				
				<include content="submenu_list" condition="Window.IsVisible(1157)">	
					<param name="id" value="9010" />
					<param name="left" value="-900" />
					<param name="top" value="360" />
					<param name="width" value="900" />
					<param name="height" value="1800" />
					<param name="onleft" value="Dialog.Close(submenu)" />
					<param name="onright" value="SetFocus(4400)" />
					<param name="align" value="left" />
					<param name="focusposition" value="0" />
					<param name="itemwidth" value="900" />
					<param name="itemheight" value="90" />
				</include>	
				<include content="submenu_content">	
				</include>				
			</control>
			<include content="context_furniture">	
				<param name="left" value="1992" />
				<param name="top" value="250" />
				<param name="label" value="31849" />
				<param name="visible" value="Window.IsVisible(home)" />
			</include>
			<control type="list" id="9011">
				<include content="context_list" condition="Window.IsVisible(home)">
					<param name="id" value="9011" />
					<param name="left" value="1992" />
					<param name="top" value="250" />					
					<param name="width" value="1362" />
					<param name="height" value="2010" />
					<param name="onup" value="9011" />
					<param name="ondown" value="9011" />
					<param name="onleft" value="9010" />
					<param name="onright" value="Dialog.Close(submenu)" />
					<param name="focusposition" value="0" />
					<param name="itemwidth" value="1362" />
					<param name="itemheight" value="90" />
				</include>			
				<content>
					<item>
						<label>31144</label>
						<onclick>ShutDown()</onclick>
					</item>
					<item>
						<label>31012</label>
						<onclick>Quit()</onclick>
					</item>
					<item>
						<label>31058</label>
						<onclick>PowerDown()</onclick>
					</item>
					<item>
						<label>31241</label>
						<onclick>System.ExecWait("/usr/sbin/rebootfromnand")</onclick>
						<visible>[System.Platform.Android | System.Platform.Linux | System.Platform.Linux.RaspberryPi] + System.CanReboot</visible>
					</item>
					<item>
						<label>31207</label>
						<onclick>Dialog.Close(submenu)</onclick>
						<onclick>ActivateWindow(1157)</onclick>
						<visible>$EXP[kiosk_code_control]</visible>
					</item>
					<item>
						<label>31371</label>
						<onclick>Dialog.Close(submenu)</onclick>
						<onclick>ActivateWindow(skinsettings)</onclick>
						<visible>$EXP[kiosk_code_control]</visible>
					</item>
					<item>
						<label>31362</label>
						<onclick>Dialog.Close(submenu)</onclick>
						<onclick>ActivateWindow(settings)</onclick>
						<visible>$EXP[kiosk_code_control]</visible>
					</item>
					<item>
						<label>$VAR[kiosk_code_label]</label>
						<onclick condition="!String.IsEqual(skin.string(kiosk_code_step),step3)">Dialog.Close(submenu)</onclick>
						<onclick condition="!String.IsEqual(skin.string(kiosk_code_step),step3)">ActivateWindow(1151)</onclick>
						<onclick condition="String.IsEqual(skin.string(kiosk_code_step),step3)">Skin.Reset(kiosk_code_step)</onclick>
						<visible>Skin.HasSetting(kiosk_code)</visible>
					</item>	
				</content>
			</control>
		</control>
	</controls>
</window>